[program:example-queue-worker]
command=php /var/www/queue-practice/bin/console consume:messages example_queue
process_name=%(program_name)s_%(process_num)02d
numprocs=2
directory=/var/www/queue-practice
autostart=true
autorestart=true
startsecs=5
startretries=10
user=www-data
redirect_stderr=true
stdout_logfile=/var/log/supervisor/example-queue-worker.log
stdout_logfile_maxbytes=10MB
stdout_logfile_backups=10
stopwaitsecs=60
stopsignal=TERM
